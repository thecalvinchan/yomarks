{
  "name": "YoClip",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Yo clipboard on yo phone",
  "homepage_url": "http://calvinchan.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "YoClip",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "https://yoclip.herokuapp.com/",
    "http://yoclip.herokuapp.com/"
  ]
}
